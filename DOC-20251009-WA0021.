.hover\:-translate-y-1:hover { --tw-translate-y: -0.25rem; }

.hover\:-translate-y-1:hover, .hover\:scale-\[1\.01\]:hover { transform: translate(var(--tw-translate-x),var(--tw-translate-y)) rotate(var(--tw-rotate)) skewX(var(--tw-skew-x)) skewY(var(--tw-skew-y)) scaleX(var(--tw-scale-x)) scaleY(var(--tw-scale-y)); }

